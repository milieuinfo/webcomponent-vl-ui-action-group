{
  "name": "vl-ui-action-group",
  "version": "1.0.14",
  "description": "Toon meerdere knoppen of links. De groep zorgt ervoor dat ze correct zijn uitgelijnd.",
  "main": "vl-action-group.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-action-group.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-action-group/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-action-group#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-action-group.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-action-group.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-action-group/style.css' vl-action-group.js",
    "release:prepare:commit": "git add -f vl-action-group.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "1.4.4"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2"
  },
  "devDependencies": {
    "@govflanders/vl-ui-action-group": "3.7.2",
    "@govflanders/vl-ui-button": "3.7.2",
    "@govflanders/vl-ui-core": "3.7.2",
    "@govflanders/vl-ui-link": "3.7.2",
    "@govflanders/vl-ui-util": "3.7.2",
    "http-server": "0.11.1",
    "np": "3.0.4",
    "replace": "1.1.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-button": "1.1.3",
    "vl-ui-icon": "2.0.6",
    "vl-ui-link": "1.1.4",
    "vl-ui-util": "^1.2.22",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  }
}
